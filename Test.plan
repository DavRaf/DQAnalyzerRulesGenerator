			</domains>
			<fkAnalysis/>
			<inputs>
				<profilingInput name="party" drillThrough="true" drillThroughLimit="200">
					<businessRules/>
					<dataToProfile>
						<profiledData expression="src_name" maskName="words" analyseBusinessDomain="false" analyseDomain="true">
							<frequencyAnalysis calculate="true"/>
